#include "rgbapixel.h"
#include <cstdint>
using std::uint8_t;

RGBAPixel::RGBAPixel()
{
        red = 255;
        green = 255;
        blue = 255;
        alpha = 255;
}
RGBAPixel::RGBAPixel(uint8_t r, uint8_t g,uint8_t b) {
        this -> red = r;
        this -> green = g;
        this -> blue = b;
        this -> alpha = 255;
}

